{
  "name": "@kspace-trk/vercel-remix-adapter",
  "version": "2.16.6",
  "description": "Isomorphic runtime + adapter for Remix on Vercel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kspace-trk/remix.git",
    "directory": "packages/vercel-remix"
  },
  "author": "kspace-trk",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "browser": "dist/edge/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/edge/index.js",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./server.d.ts",
      "browser": "./dist/edge/server.js",
      "default": "./server.js"
    },
    "./vite": {
      "types": "./vite.d.ts",
      "default": "./vite.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@vercel/static-config": "3.0.0",
    "isbot": "^3.6.8",
    "ts-morph": "12.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "2.16.6",
    "@remix-run/node": "2.16.6",
    "@remix-run/server-runtime": "2.16.6",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@remix-run/dev": "2.16.6",
    "@remix-run/node": "2.16.6",
    "@remix-run/server-runtime": "2.16.6",
    "react": "*",
    "react-dom": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "defaults/",
    "entry-server-shas.json",
    "globals.js",
    "globals.d.ts",
    "server.js",
    "server.d.ts",
    "vite.js",
    "vite.d.ts",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ]
}
